  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./build/bin/www.js",
    "start:dev": "cross-env NODE_ENV=development nodemon ./server/bin/www.js --exec babel-node -e js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf build && cross-env NODE_ENV=production babel server/ -d build --copy-files",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "pack": "webpack --config webpack.prod.js",
    "client": "webpack-dev-server --config webpack.dev.js",
    "dev": "concurrently \"npm run start:dev\" \"npm run client\""
  },
